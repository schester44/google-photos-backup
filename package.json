{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "swc-node src/index.ts",
    "dev": "nodemon --exec 'swc-node' src/index.ts",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "docker-google-photos-backup",
  "author": "Steve Chester",
  "module": "dist/docker-google-photos-backup.esm.js",
  "size-limit": [
    {
      "path": "dist/docker-google-photos-backup.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/docker-google-photos-backup.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.1.0",
    "@swc/core": "^1.3.24",
    "@types/express": "^4.17.15",
    "husky": "^8.0.2",
    "size-limit": "^8.1.0",
    "swc-node": "^1.0.0",
    "ts-node": "^10.9.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "axios": "^1.2.1",
    "chokidar": "^3.5.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "get-port": "^6.1.2",
    "googleapis": "^110.0.0",
    "lowdb": "^5.0.5",
    "nodemon": "^2.0.20",
    "uuid": "^9.0.0"
  }
}
